{
  "name": "eslint-plugin-flowtype-errors",
  "version": "2.0.1-0",
  "description": "Retrieve Flow errors as ESLint errors.",
  "author": "Amila Welihinda",
  "bugs": {
    "url": "https://github.com/amilajack/eslint-plugin-flowtype-errors/issues"
  },
  "homepage": "https://github.com/amilajack/eslint-plugin-flowtype-errors#readme",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir dist",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty src test *.js",
    "preversion": "npm run build",
    "spec": "npm run build && cross-env NODE_ENV=test mocha --inline-diffs --async-only --growl --compilers js:babel-register --recursive test/*.spec.js",
    "test": "npm run lint && npm run spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/eslint-plugin-flowtype-errors.git"
  },
  "keywords": [
    "eslint",
    "flow",
    "plugin",
    "errors"
  ],
  "files": [
    "index.js",
    "dist"
  ],
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "q": "^1.4.1",
    "ramda": "^0.22.1",
    "shelljs": "^0.7.5",
    "slash": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-nibble-ignore": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.8.0",
    "flow-bin": "^0.36.0",
    "mocha": "^3.2.0"
  },
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3.x"
  },
  "peerDependencies": {
    "flow-bin": ">=0.36.0",
    "eslint": ">=3.0.0"
  }
}
